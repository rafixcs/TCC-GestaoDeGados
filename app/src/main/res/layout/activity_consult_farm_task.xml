<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.farm.ConsultFarmTaskActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/registerFarmTaskFab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/va_register_bovine"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewTaskFarm"
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/emptyTaskListTextFarm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/empty_task_list"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerViewTaskFarm"
        app:layout_constraintEnd_toEndOf="@+id/recyclerViewTaskFarm"
        app:layout_constraintStart_toStartOf="@+id/recyclerViewTaskFarm"
        app:layout_constraintTop_toTopOf="@+id/recyclerViewTaskFarm" />
</androidx.constraintlayout.widget.ConstraintLayout>